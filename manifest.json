{
  "manifest_version": 2,

  "name": "Scalped",
  "description": "Gets rid of annoying ticket scalper postings on Craigslist",
  "version": "0.1",
  "icons": { "16": "icon_16.png",
           "48": "icon_48.png",
          "128": "icon_128.png" },
  "options_page": "options.html",
  "permissions": [
    "storage"
    ],
  "content_scripts": [
    {
      "matches": ["http://*.craigslist.ca/sss/*"],
      "css": ["extension.css"],
      "js": ["jquery.js", "extension.js"]
    }
  ]
}